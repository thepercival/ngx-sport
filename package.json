  "scripts": {
    "build": "ng-packagr -p ng-package.json -c tsconfig.json",
    "build-fct": "npm run build && npm run copy-to-fct",
    "build-sup": "npm run build && npm run copy-to-sup",
    "build-vob": "npm run build && npm run copy-to-vob",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "copy-to-fct": "rm -rf ../fctoernooi/node_modules/ngx-sport && cp -r dist/ ../fctoernooi/node_modules/ && mv ../fctoernooi/node_modules/dist/ ../fctoernooi/node_modules/ngx-sport/ && cp -r dist/fesm2020 ../fctoernooi/node_modules/ngx-sport",
    "copy-to-sup": "rm -rf ../superelf/node_modules/ngx-sport && cp -r dist/ ../superelf/node_modules/ && mv ../superelf/node_modules/dist/ ../superelf/node_modules/ngx-sport/ && cp -r dist/fesm2020 ../superelf/node_modules/ngx-sport",
    "test": "mocha --loader=ts-node/esm 'test/cases/**/*.{ts,js}'",
    "coverage": "nyc npm run test",
    "profile": "ngc -p tsconfig-profile.json",
    "help": "nyc help"
  },
  "sideEffects": false,
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/mapper.ts",
      "src/**/json.ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage/reports",
    "temp-dir": "coverage/.nyc_output"
  }
}