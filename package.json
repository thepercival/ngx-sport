  "main": "./bundles/ngx-sport.umd.js",
  "module": "./esm5/ngx-sport.js",
  "es2015": "./esm2015/ngx-sport.js",
  "typings": "./ngx-sport.d.ts",
  "maintainers": [
    {
      "name": "thepercival",
      "email": "coendunnink@gmail.com"
    }
  ],
  "name": "ngx-sport",
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/thepercival/ngx-sport.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:inline && npm run build:esm2015 && npm run build:fesm2015 && npm run build:esm5 && npm run build:fesm5 && npm run build:umd && npm run build:umd-min && npm run move:assets",
    "build:inline": "mkdir tmp && cp *.ts ./tmp && cp -r ./src ./tmp",
    "build:esm2015": "ngc -p tsconfig-esm2015.json",
    "build:fesm2015": "rollup -c rollup-esm2015.conf.js",
    "build:esm5": "ngc -p tsconfig-esm5.json",
    "build:fesm5": "rollup -c rollup-esm5.conf.js",
    "build:umd": "rollup -c rollup-umd.conf.js",
    "build:umd-min": "uglifyjs dist/bundles/ngx-sport.umd.js -c -m -o dist/bundles/ngx-sport.umd.min.js",
    "move:assets": "cp -r tmp/esm2015/*.d.ts tmp/esm2015/src tmp/esm2015/*.json package.json README.md dist/ && rm dist/src/*.js",
    "clean": "rm -rf ./tmp && rm -rf ./dist",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "build:dev:fct": "rm -rf ../fctoernooi/node_modules/ngx-sport && cp -r dist/ ../fctoernooi/node_modules/ && mv ../fctoernooi/node_modules/dist/ ../fctoernooi/node_modules/ngx-sport/",
    "build:dev:vob": "rm -rf ../vobetting/node_modules/ngx-sport && cp -r dist/ ../vobetting/node_modules/ && mv ../vobetting/node_modules/dist/ ../vobetting/node_modules/ngx-sport/",
    "test": "mocha -r test/hook.js --recursive test/cases/**/*.ts"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "getLastRelease": "@semantic-release/npm",
    "prepare": [
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json"
        ]
      }
    ],
    "publish": [
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      }
    ]
  },
  "version": "0.0.75"
}
