    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "codelyzer": "^5.2.2",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "mocha": "^7.1.1",
    "nyc": "github:istanbuljs/nyc",
    "rollup": "^2.6.1",
    "semantic-release": "^17.0.6",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.8.2",
    "tsickle": "^0.38.1",
    "typescript": "3.8.3",
    "typescript-eslint": "0.0.1-alpha.0",
