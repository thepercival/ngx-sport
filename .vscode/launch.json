{
    "version": "0.1.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha Tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "-u",
                "tdd",
                "--timeout",
                "999999",
                "--require",
                "${workspaceFolder}/test/hook.js",
                "--colors",
                "--recursive",
                "${workspaceFolder}/test/cases/ranking/end/service.ts"
            ],
            "env": {
                "NODE_ENV": "development",
            },
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "profiler",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "env": {
                "TS_NODE_PROJECT": "tsconfig-profile.json"
            },
            "args": [
                "${workspaceFolder}/test/profile/planning.ts"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "profilerjs",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "env": {
                "TS_NODE_PROJECT": "tsconfig-profile-2.json"
            },
            "args": [
                "${workspaceFolder}/test/profile/planning.ts"
            ]
        }
    ]
}